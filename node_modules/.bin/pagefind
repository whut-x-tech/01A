#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/dinghaoran02/Documents/lab/01A/node_modules/.pnpm/pagefind@1.3.0/node_modules/pagefind/lib/runner/node_modules:/Users/dinghaoran02/Documents/lab/01A/node_modules/.pnpm/pagefind@1.3.0/node_modules/pagefind/lib/node_modules:/Users/dinghaoran02/Documents/lab/01A/node_modules/.pnpm/pagefind@1.3.0/node_modules/pagefind/node_modules:/Users/dinghaoran02/Documents/lab/01A/node_modules/.pnpm/pagefind@1.3.0/node_modules:/Users/dinghaoran02/Documents/lab/01A/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/dinghaoran02/Documents/lab/01A/node_modules/.pnpm/pagefind@1.3.0/node_modules/pagefind/lib/runner/node_modules:/Users/dinghaoran02/Documents/lab/01A/node_modules/.pnpm/pagefind@1.3.0/node_modules/pagefind/lib/node_modules:/Users/dinghaoran02/Documents/lab/01A/node_modules/.pnpm/pagefind@1.3.0/node_modules/pagefind/node_modules:/Users/dinghaoran02/Documents/lab/01A/node_modules/.pnpm/pagefind@1.3.0/node_modules:/Users/dinghaoran02/Documents/lab/01A/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/pagefind@1.3.0/node_modules/pagefind/lib/runner/bin.cjs" "$@"
else
  exec node  "$basedir/../.pnpm/pagefind@1.3.0/node_modules/pagefind/lib/runner/bin.cjs" "$@"
fi
