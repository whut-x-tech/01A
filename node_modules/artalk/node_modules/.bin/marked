#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/lucifer/OneDrive/Desktop/01A/node_modules/.pnpm/marked@14.1.4/node_modules/marked/bin/node_modules:/mnt/c/Users/lucifer/OneDrive/Desktop/01A/node_modules/.pnpm/marked@14.1.4/node_modules/marked/node_modules:/mnt/c/Users/lucifer/OneDrive/Desktop/01A/node_modules/.pnpm/marked@14.1.4/node_modules:/mnt/c/Users/lucifer/OneDrive/Desktop/01A/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/lucifer/OneDrive/Desktop/01A/node_modules/.pnpm/marked@14.1.4/node_modules/marked/bin/node_modules:/mnt/c/Users/lucifer/OneDrive/Desktop/01A/node_modules/.pnpm/marked@14.1.4/node_modules/marked/node_modules:/mnt/c/Users/lucifer/OneDrive/Desktop/01A/node_modules/.pnpm/marked@14.1.4/node_modules:/mnt/c/Users/lucifer/OneDrive/Desktop/01A/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../marked@14.1.4/node_modules/marked/bin/marked.js" "$@"
else
  exec node  "$basedir/../../../../../marked@14.1.4/node_modules/marked/bin/marked.js" "$@"
fi
